/**
* Modificar el fichero para incluir las reglas que se consideren necesarias
*/
// **RECUERDA** PRIORIDAD EN FUNCIÓN DEL ORDEN DE LAS REGLAS (LA SUPERIOR LA MÁS PRIORITARIA)
rule prioridad1 {  // si las dos variables del objeto son iguales entonces las variables son "ejemplo" y "acabado"
  declarations
    inferencia.practica.Objeto1 O;    
  preconditions
    O.getVariable1().equals(O.getVariable2());		
  actions
 	O.setVariable1("ejemplo");
 	O.setVariable2("acabado");
 	//no se incluye el modified para forzar que finalice la ejecución
}
rule prioridad2 {  // si las dos variables del objeto son distintas entonces las igualamos
  declarations
    inferencia.practica.Objeto1 O;    
  preconditions
    !O.getVariable1().equals(O.getVariable2());		
  actions
 	O.setVariable1(O.getVariable2());
    modified(O);	// Indica que ha cambiado y por tanto se puede volver a utilizar para otra regla
}







